#: addon\globalPlugins\browserNav\__init__.py:1632
msgid "No previous dialog"
msgstr "Немає попереднього діалогу"

#: addon\globalPlugins\browserNav\__init__.py:1633
msgid "Jump to previous dialog"
msgstr "Перейти до попереднього діалогу"

#: addon\globalPlugins\browserNav\__init__.py:1651
msgid "No next menu"
msgstr "Немає наступного меню"

#: addon\globalPlugins\browserNav\__init__.py:1654
msgid "Jump to next menu"
msgstr "Перейти до наступного меню"

#: addon\globalPlugins\browserNav\__init__.py:1661
msgid "No previous menu"
msgstr "Немає попереднього меню"

#: addon\globalPlugins\browserNav\__init__.py:1664
msgid "Jump to previous menu"
msgstr "Перейти до попереднього меню"

#: addon\globalPlugins\browserNav\__init__.py:1673
